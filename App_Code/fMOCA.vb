Imports Microsoft.VisualBasic
Imports System.Data

Public Class fMOCA
    Public Shared Function CheckPageAccess(ByVal pPageNumber As String) As DataTable
        Dim ws As New MOCA.SecurityCredentialsSoapClient
        Dim dt As DataTable
        dt = ws.CheckPageAccess(My.User.Name, 
                                pPageNumber, 
                                ConfigurationManager.AppSettings.Item("AppTOKEN"))
        Return dt
    End Function
End Class
